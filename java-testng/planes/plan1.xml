<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Plan" verbose="1">
	<test name="VerificarClientesNuevos">
		<classes>
			<class
				name="io.github.jschenfeld.automation.exercise1.RegistroClientesTest">
				<methods>
					<include name="agregarClientesTest" />
					<include name="obtenerClientesTest" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="BorrarClienteExistente">
		<classes>
			<class
				name="io.github.jschenfeld.automation.exercise1.RegistroClientesTest">
				<methods>
					<parameter name="dni" value="2" />
					<include name="eliminarClientesTest" />
				</methods>
			</class>
		</classes>
	</test>
</suite>